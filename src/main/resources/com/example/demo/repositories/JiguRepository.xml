<!DOCTYPE mapper
    PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
    "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.example.demo.repositories.JiguRepository">
   <insert id="insert" parameterType="com.example.demo.domains.JiguDto">
   <!-- writerId, title, content, regDate, count, jiguNum -->
        insert into jigus(writer_id, title, content, reg_date,  count)
        values (#{writerId}, #{title}, #{content}, ${regDate}, #{count})
    </insert>
   
</mapper>
